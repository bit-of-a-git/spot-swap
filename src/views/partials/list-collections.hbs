{{#each collections }}
  <div class="box box-link-hover-shadow">
    <h2 class="title">{{title}}</h2>
    <a href="/collection/{{_id}}" class="ui icon button is-link">
      <i class="fas fa-folder-open"></i>
    </a>
    <a href="/dashboard/deletecollection/{{_id}}"
       class="ui icon button is-danger"
       title="Delete Collection">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}
